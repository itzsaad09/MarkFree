{
  "version": 2,
  "builds": [
    {
      "src": "api/remove_watermark.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "100mb",
        "functions": {
          "api/remove_watermark.py": {
            "memory": 3008,
            "maxDuration": 60
          }
        }
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/remove_watermark.py" }
  ]
}